client
dev <%= @dev %>
proto <%= @proto %>
remote <%= @server %> <%= @port %>
resolv-retry infinite
nobind
persist-key
persist-tun
ca <%= @ca %>
cert <%= @cert %>.crt
key <%= @cert %>.key
<% if @tls_auth_key -%>
tls-auth <%= @tls_auth_key %> 1
<% end -%>
<% if @openvpn_user -%>
user  <%= @openvpn_user %>
<% end -%>
<% if @openvpn_group -%>
group <%= @openvpn_group %>
<% end -%>
ns-cert-type <%= @ns_cert_type %>
cipher <%= @cipher %>
<%if @compression -%>
comp-lzo
<% end -%>
verb <%= @verb %>
# custom options
<% @custom_options.each do |custom_option| -%>
<%= custom_option %>
<% end -%>
